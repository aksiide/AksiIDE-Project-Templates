<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aksiide.hello" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>%project.name%</name>
    <description>
        A sample Cordova application generated with AksiIDE.
    </description>
    <author email="support@aksiide.com" href="http://www.aksiide.com">
        AksiIDE Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Sms">
        <param name="android-package" value="com.jsmobile.plugins.sms.Sms" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
</widget>
